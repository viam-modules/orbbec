{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "viam:orbbec",
  "visibility": "public",
  "url": "https://github.com/viam-modules/orbbec",
  "description": "C++ camera module for Orbbec cameras",
  "models": [
    {
      "api": "rdk:component:camera",
      "markdown_link": "README.md#model-viamorbbecastra2",
      "model": "viam:orbbec:astra2",
      "short_description": "Orbbec Astra2 driver return images, depth maps and pointclouds"
    },
    {
      "api": "rdk:service:discovery",
      "markdown_link": "README.md#model-viamorbbecdiscovery",
      "model": "viam:orbbec:discovery",
      "short_description": "A discovery service to find connected Orbbec cameras"
    }
  ],
  "entrypoint": "bin/orbbec-module",
  "first_run": "./first_run.sh"
}
